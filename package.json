{
  "name": "dashboard-tenjo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "packages/*"
  ],
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:shared": "cd packages/shared && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../packages/shared && npm install"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  },
  "dependencies": {
    "install": "^0.13.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  }
}
